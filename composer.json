{
  "name": "doc/jatsparser",
    "description": "JATS XML parser",
    "type": "library",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Vitaliy Bezsheiko",
            "email": "vitaliybezsh@gmail.com"
        }
    ],
    "minimum-stability": "dev",
